{
    "name": "skumar/cmpdate",
    "authors": [
        {
            "name": "Satish Kumar",
            "email": "satish.prg@gmail.com"
        }
    ],
    "require": {
        "slim/slim": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
      "classmap": [
          "cmpdate"
        ],
        "psr-4": {
            "CmpDate\\" : "cmpdate/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
